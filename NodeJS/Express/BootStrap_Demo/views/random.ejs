<%- include('partials/head.ejs')%>
<body>

    <%- include('partials/nav.ejs')%>

    <h1>Your Random Number is: <%= rand %></h1>
    <% if(rand%2===0){ %>
        <h2>It is a Even Number</h2>
    <% } else{ %>
        <h2>It is a Odd Number</h2>
    <% } %>
    <!-- or -->
    <h3>That number is: <%= rand%2===0 ? 'Even':'ODD' %> </h3>
</body>
</html>